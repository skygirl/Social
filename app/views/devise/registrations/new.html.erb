
<div class="row">
  <div class="span4">
    
        <%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: {class: "well"}) do |f| %>
  <%= devise_error_messages! %>
  <fieldset>
    <legend><h2>Sign Up!</h2></legend>
    <div><%= f.input :first_name %><br />
    <div><%= f.input :last_name %><br />
  	
  	<div><%= f.input :profile_name %><br />
  
  <div><%= f.input :email %><br />

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.button :submit, "Sign up" %></div>
</fieldset>
<% end %>

<%= render "devise/shared/links" %>
